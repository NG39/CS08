enum Role {
  Admin
	Normal
}

type User {
  id: ID! @id
  name: String!
  email: String! @unique
  password: String!
  role: Role!
}

type Post {
	id: ID! @id
	text: String! @default(value: "")
	createdAt: DateTime! @createdAt
	updatedAt: DateTime! @updatedAt
	postedBy: User! @relation(link: INLINE)
}